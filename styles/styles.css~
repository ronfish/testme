    .owl-core {
      margin: 1px 0 24px; } }
  .owl-core::before, .owl-core::after {
    content: '';
    display: none;
    position: absolute;
    top: 0;
    width: 24px;
    height: 100%;
    z-index: 1; }
    @media only screen and (min-width: 768px) {
      .owl-core::before, .owl-core::after {
        display: block; } }
  .owl-core::before {
    left: 0;
    text-align: right;
    background-color: transparent;
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, white), color-stop(100%, rgba(255, 255, 255, 0)));
    background-image: -webkit-linear-gradient(left, white, rgba(255, 255, 255, 0));
    background-image: linear-gradient(to right,white, rgba(255, 255, 255, 0)); }
  .owl-core::after {
    right: 0;
    text-align: left;
    background-color: transparent;
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, white), color-stop(100%, rgba(255, 255, 255, 0)));
    background-image: -webkit-linear-gradient(right, white, rgba(255, 255, 255, 0));
    background-image: linear-gradient(to left,white, rgba(255, 255, 255, 0)); }
